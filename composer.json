{
    "name": "aic/data-service-catalogues",
    "description": "A data service for the AIC data hub.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/art-institute-of-chicago/data-hub-foundation.git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.8.*",
        "aic/data-hub-foundation": "dev-master#dd91c35745b6b61607b0f558dbba984bc0af96ca",
        "league/csv": "^9.1",
        "league/html-to-markdown": "^4.6",
        "symfony/dom-crawler": "^3.3",
        "wa72/htmlpagedom": "^1.3",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
