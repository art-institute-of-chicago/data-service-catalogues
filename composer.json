{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/art-institute-of-chicago/data-hub-foundation.git"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "^5.5",
        "aic/data-hub-foundation": "dev-master#ccb35d9a7aa38dd84d80a8e4b46cb3b38f559f39",
        "league/csv": "^9.1",
        "league/flysystem": "^1.0",
        "graham-campbell/flysystem": "^4.0",
        "league/html-to-markdown": "^4.6",
        "symfony/dom-crawler": "^3.3",
        "wa72/htmlpagedom": "^1.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
